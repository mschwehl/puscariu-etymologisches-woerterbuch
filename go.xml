    <cognates>
      <form lang="vec">nevodo?</form>, <!-- Interpretation needed for nelv)odo -->
      <form lang="ca">nebot?</form>, <!-- Interpretation needed for nebodo -->
    </cognates> <etymon lang="Lat">NEPOTA, -AM</etymon> (Dens. H. l. r. 135):
    <cognates>
      <form lang="vec">nevoda</form>, <form lang="lmo">nevoda</form>, <!-- Interpretation needed for lomb. -->
