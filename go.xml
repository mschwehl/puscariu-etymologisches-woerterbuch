    <cognates>
      <form lang="sc-LOG">andzone</form> „einjähr. Lamm“. <!-- Interpretation needed for log. -->
    </cognates>
    — <etymon lang="Lat">*ANNOTICUS, -UM</etymon>:
    <cognates>
      <form lang="it-SIC">annòticu?</form>, <!-- Interpretation needed for sie. annoticu -->
      <form lang="fr">(dial.) annoge</form> „einjähr. Rind“, <form lang="fr">noge</form> (<form lang="fr">l'anoge</form>) „genisse d'un an“, <!-- Interpretation needed for Vanoge = la noge -->
