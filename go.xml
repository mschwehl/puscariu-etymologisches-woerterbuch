      <form lang="it-ABR">annicchio?</form> „einjähr. Ziege“, <!-- Interpretation needed for nnekie -->
      <form lang="es">añejo</form>. <!-- Assuming afejo -> añejo -->
    </cognates>
    — <etymon lang="Lat">*ANNUCULUS, -UM</etymon>:
    <cognates>
      <form lang="roh-OBW">anugl?</form> „Widder“, <!-- Interpretation needed for anu? -->
      <form lang="es">añojo</form> „einjähr. Rind“. <!-- Assuming afiojo -> añojo -->
    </cognates>
    — <etymon lang="Lat">*ANNICELLA, -AM</etymon>:
    <cognates>
      <form lang="lmo-TES">nesela?</form> „Junge Ziege“, <!-- Interpretation needed for tessin. nezela -->
    </cognates>
