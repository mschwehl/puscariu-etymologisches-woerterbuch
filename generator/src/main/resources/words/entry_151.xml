<entry number="151">
  
  <lemma>aștern</lemma>
   
  <!-- Assuming astern -> aștern -->
  
  <inflection>-ui, -ut</inflection>
   
  <!-- Assuming -ıti, -üt -> -ui, -ut -->
  
  <verb_class>III</verb_class>
  
  <pos>vb.</pos>
  
  <variants>[ar. așternu, așteru in Albanien, mgl. ștern, ir. (a)șternu]</variants>
   
  <!-- Assuming asternu, asteru -> așternu, așteru; Stern -> ștern; (a)Sternu -> (a)șternu -->
  
  <definition>„hinstreuen, das Bett machen“</definition>
  
  <etymology>
    
    <etymon lang="Lat">ASTERNO, -STRĀVI, -STRĀTUM, -STERNERE</etymon>
