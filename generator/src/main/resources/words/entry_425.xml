  <lemma>cuceresc</lemma>
   
  <!-- Assuming cucerese -> cuceresc -->
  
  <verb_class>IV</verb_class>
  
  <pos>vb.</pos>
  
  <definition>„erobern“</definition>
  
  <etymology>
    
    <etymon lang="Lat">*CONQUAERO, -IRE</etymon>
