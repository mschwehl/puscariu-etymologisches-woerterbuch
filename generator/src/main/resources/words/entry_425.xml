    <!-- Assuming # -> * -->
    
    <cognates>
      
      <form lang="pv">conquerir</form>
      , 
      <form lang="sp">conquerir</form>
      , 
      <form lang="fr">conquérir</form>
      . 
      <!-- Assuming conguerir -> conquérir -->
      
    </cognates>
    
  </etymology>
  
  <reflexive_usage>
    Refl. 
    <form>mă cuceresc</form>
     heißt „ich demütige mich (
    <source_ref>Coresi: Tetraev. 119, 142</source_ref>
    )“ daher: 
    <form>cucerie</form>
     sf. "Demütigung“ und 
    <form>cucernic, -ă</form>
     adj. „andächtig, fromm“. 
    <!-- Assuming cucesie si. -> cucerie sf., eneörnie -> cucernic -->
    
  </reflexive_usage>
  
