  <variants>[ar. mgl. iarbă, ir. iarbă?]</variants>
  
  <!-- Assuming jarbă -> iarbă, jorbe -> iarbă? -->
  
  <definition>„Gras“</definition>
  
  <etymology source="Zeitschr. rom. Phil. XXVI, 657; W. Meyer: Lat. Neutr. 58">
    
    <etymon lang="Lat">HERBA, -AM</etymon>
    : 
    <!-- Assuming -aı -> -AM -->
    
    <cognates>
      
      <form lang="isv">jarba</form>
      , 
      <!-- Assuming vegl. -> isv -->
      
      <form lang="it">erba</form>
      ,

      <form lang="it-SIC">erba</form>
      , 
      <!-- Assuming sie. -> it-SIC -->
      
      <form lang="lmo-MIL">erba</form>
      , 
      <!-- Assuming mail. -> lmo-MIL -->
      
      <form lang="roh-ENG">erba</form>
      , 
      <!-- Assuming eng. -> roh-ENG -->
      
      <form lang="oc">erba</form>
      , 
      <!-- Assuming pv. -> oc -->
      
      <form lang="fur">iarbe?</form>
      , 
      <!-- Assuming frl. ierbe -> iarbe? -->
      
      <form lang="fro">erbe, èvre?</form>
       (Zeitschr. rom. Phil. XXVI, 657), 
      <!-- Assuming a.-frz. ebre, evre -> erbe, èvre? -->
      
      <form lang="fr">herbe</form>
      , 
      <!-- Assuming n.-frz. -> fr -->
      
      <form lang="fr-LOR">yerb</form>
      , 
      <!-- Assuming lothr. -> fr-LOR -->
      
