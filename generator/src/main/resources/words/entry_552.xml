<entry number="552">
  
  <lemma>duc</lemma>
   
  <!-- Assuming due -> duc -->
  
  <inflection>dusei (ban. + dus), dus</inflection>
  
  <verb_class>III</verb_class>
  
  <pos>vb.</pos>
  
  <variants>[ar. duc, dus, dus III, II, mgl. duc?, dus, dus III, ir. duc?, dus, dus III]</variants>
   
